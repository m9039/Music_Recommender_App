<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
                <size
                    android:height="70dp"
                    android:width="70dp" />
                <solid
                    android:color="#FF8837"/>
                <corners
                    android:radius="35dp"/>
        </shape>
    </item>

    <item
        android:top="10dp"
        android:left="10dp"
        android:right="60dp"
        android:bottom="10dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#ffff00" />
            </shape>
        </rotate>
    </item>

    <item
        android:top="55dp"
        android:left="55dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <size
                android:height="20dp"
                android:width="20dp" />
            <solid android:color="#323232" />
            <corners
                android:radius="15dp"/>

        </shape>
    </item>

    <include
        layout = "@drawable/ic_fastforward"
        />


</layer-list>